<html lang="en-US">

<head>
    <!-- META TAGS -->
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Dots Forum</title>

    <!-- Style Sheet-->
    <link rel="stylesheet" id="bootstrap-css-css" href="/css/bootstrap5152.css?ver=1.0" type="text/css" media="all">
    <link rel="stylesheet" id="main-css-css" href="/css/main5152.css?ver=1.0" type="text/css" media="all" >
      <link rel="stylesheet" id="responsive-css-css" href="/css/responsive5152.css?ver=1.0" type="text/css" media="all">

</head>

<body>


    <!-- header -->
    <div class="header-wrapper">
        <header>
            <div class="container">

                <div class="logo-container">
                    <span class="tag-line">Dots Forum: A perfect platform for industry information sharing</span>
                </div>

                <!--  navigation -->
                <nav class="main-nav">
                    <div class="menu-top-menu-container">
                        <ul id="menu-top-menu" class="clearfix">
                            <li><a href="/">Home</a></li>
                            <li><a href="/about">About Page</a></li>
                            <li><a href="/loggedin/createpost">Ask Question</a></li>
                            <li><a href="/login">Log In</a></li>
                            <li><a href="/register">Register</a></li>
                            <li><a href="/contact">Contact Page</a></li>
                            <li><a href="/promotion">Promotion Page</a></li>

                        </ul>
                    </div>
                </nav>
                <!--  navigation ends-->

            </div>
        </header>
    </div>
    <!-- header ends -->

    <!-- Search Wrapper -->
    <div class="search-area-wrapper">

        <div class="search-area container">
            <h3 class="search-header">Need Answers for Questions Related to the Industry?</h3>
            <p class="search-tag-line">Simply search and get insights here!</p>
            <form id="search-form" class="search-form clearfix" method="get" action="/search" autocomplete="off">
                <input class="search-term required" type="text" id="s" name="s" placeholder="Type your key word here"
                    title="* Please enter a search term!">
                <input class="search-btn" type="submit" value="Search">
                <div id="search-error-container"></div>
            </form>
        </div>
    </div>
    <!-- Search Wrapper ends -->

    <!-- Start of Page Container -->
    <div class="page-container">
        <div class="container">
            <div class="row">

                <!-- start of page content -->
                <div class="span8 main-listing">
                                        <h6 class="post-title">**These are posts that have titles that fits the search content.</h6>
                    <article class=" type-post format-image hentry clearfix">

                        <header class="clearfix">
                            <ul class="articles">
                                <% for(var i = addedpost.length-1; i >=0;i--){ %>
                                <li class="article-entry standard">
                                    <h4><a href="/post/<%=addedpost[i]._id%>"> <%= addedpost[i].title %>
                                            <%= addedpost[i].posttype %></a></h4>
                                    <span class="article-meta">Asked By <span
                                            id="authorname"><%= addedpost[i].name %></span></span>
                                </li>
                                <% } %>
                            </ul>

<!--
                            <div class="post-meta clearfix">
                                <span class="date">25 Feb, 2013</span>
                                <span class="category"><a href="#"
                                        title="View all posts in Designing in WordPress">Designing in
                                        WordPress</a></span>
                                <span class="comments"><a href="#" title="Comment on Using Images">0 Comments</a></span>
                                <span class="like-count">7</span>
                            </div>end of post meta -->

                        </header>
                    </article>
                    <div id="pagination">
                        <a href="#" class="btn active">1</a>
                        <a href="#" class="btn">2</a>
                        <a href="#" class="btn">3</a>
                        <a href="#" class="btn">Next »</a>
                        <a href="#" class="btn">Last »</a>
                    </div>
            
                </div>
                <!-- end of page content -->


                <!-- start of sidebar -->
                <aside class="span4 page-sidebar">

                    <section class="widget">
                        <div class="support-widget">
                            <h3 class="title">Dots Forum Organizers</h3>
                            <p class="intro">If you have questions related to the Dots Forum, click <a
                                    href="/askexpert">here</a>.</p>
                        </div>
                    </section>

                    <section class="widget">
                        <div class="quick-links-widget">
                            <h3 class="title">Quick Links</h3>
                            <ul id="menu-quick-links" class="menu clearfix">
                                <li><a href="/">Home</a></li>
                                <li><a href="/loggedin/createpost">Ask Question</a></li>
                                <li><a href="/login">Log In</a></li>
                                <li><a href="/register">Register</a></li>

                            </ul>
                        </div>
                    </section>

                    <section class="widget">
                        <h3 class="title">Topics</h3>
                        <div class="tagcloud">
                            <a href="#" class="btn btn-mini">Finding Employment</a>
                            <a href="#" class="btn btn-mini">Tech Trend</a>
                            <a href="#" class="btn btn-mini">Venture Capital Sharing</a>
                            <a href="#" class="btn btn-mini">Start-up Ideas</a>
                            <a href="#" class="btn btn-mini">Economic predictions</a>

                        </div>
                    </section>

                </aside>
                <!-- end of sidebar -->
            </div>
        </div>
    </div>

    <!-- End of Page Container -->

    <!-- Start of Footer -->
    <footer id="footer-wrapper">
        <div id="footer" class="container">
            <div class="row">

                <div class="span3">
                    <section class="widget">
                        <h3 class="title">Why Dots Forum?</h3>
                        <div class="textwidget">
                            <p>The online Dots Forum allows for sharing of trendy industry information and communication
                                among students, start-up employees and venture capital representatives. We also
                                distributes information regarding the offline Dots Forum. One can search for the
                                answers to one's questions by entering keywords in the search bar.
                            </p>
                        </div>
                    </section>
                </div>

                <div class="span3">
                    <section class="widget">
                        <h3 class="title">Question to Organizers</h3>
                        <div id="twitter_update_list">
                            <ul>
                                <li>Do you have questions for the Dots Forum? Simply click <a href="/askexpert">here</a>
                                    to talk to our organizers!</li>
                            </ul>
                        </div>

                    </section>
                </div>

                <div class="span3">
                    <section class="widget">
                        <h3 class="title">Topics</h3>
                        <ul>
                             <li><a href="#" title="Finding Employment">Finding Employment</a> </li>
                             <li><a href="#" title="Tech Trend">Tech Trend</a></li>
                             <li><a href="#" title="Venture Capital Sharing">Venture Capital Sharing</a></li>
                             <li><a href="#" title="Start-up Ideas">Start-up Ideas</a></li>

                        </ul>
                    </section>
                </div>


                <div class="span3">
                    <section class="widget">
                        <h3 class="title">Quick Links</h3>
                        <ul>
                            <li><a href="/" title="Home">Home</a> </li>
                            <li><a href="/loggedin/createpost">Ask Question</a></li>
                            <li><a href="/login" title="Log In">Log In</a></li>
                            <li><a href="/register" title="Register">Register</a></li>
                        </ul>
                    </section>
                </div>

            </div>
        </div>
        <!-- end of #footer -->

        <!-- Footer Bottom -->
        <div id="footer-bottom-wrapper">
            <div id="footer-bottom" class="container">
                <div class="row">
                    <div class="span6">
                        <p class="copyright">
                            Copyright &copy; 2020.DotsForum All rights reserved.
                        </p>
                    </div>

                </div>
            </div>
        </div>
        <!-- End of Footer Bottom -->

    </footer>
    <!-- End of Footer -->

    <a href="#top" id="scroll-top"></a>

</body>

</html>
